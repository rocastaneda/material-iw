{
  "name": "material-iw",
  "version": "0.2.9",
  "description": "Interware styled components based on React Bootstrap and Bootstrap styles",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config build-utils/webpack.config.js --env.env=prod",
    "dev": "webpack-dev-server --config build-utils/webpack.config.js --env.env=dev",
    "lint": "eslint --ext .jsx,.js src",
    "lint:fix": "eslint --ext .jsx,.js --fix src",
    "onchange": "onchange",
    "watch-prettier": "onchange '**/*.(js|jsx)' -- prettier --write {{event}} to {{changed}}",
    "watch-client": "webpack --watch --config build-utils/webpack.config.js",
    "test": "jest --no-cache --maxWorkers=4",
    "test:verbose": "jest --verbose --no-cache --maxWorkers=4",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "prepublish": "npm run lint && npm run build"
  },
  "author": "Israel López Tiscareño",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/rocastaneda/material-iw",
    "directory": "/"
  },
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setupTestFramework.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "bootstrap-icons": "^1.0.0-alpha2",
    "cfenv": "^1.2.2",
    "cors": "^2.8.5",
    "formik": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.8.6",
    "react-select": "^3.0.8",
    "react-svg-loader": "^3.0.3",
    "style-loader": "^1.1.3",
    "underscore": "^1.9.2",
    "yup": "^0.28.0"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^5.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-module-utils": "^2.5.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.0.10",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "terser-webpack-plugin": "^2.3.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
